\mode<presentation>
\definecolor{sectionColor}{RGB}{93,190,102}
\definecolor{subsectionColor}{RGB}{157,205,134}
\definecolor{sectionTextColor}{RGB}{0,0,0}
\definecolor{subsectionTextColor}{RGB}{0,102,22}
